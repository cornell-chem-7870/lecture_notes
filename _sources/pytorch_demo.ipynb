     ]
    }
   ],
   "source": [
    "c = torch.sum(a**2 + b )\n",
    "print(c)\n",
    "a.grad = None  # Reset the gradient to None\n",
    "c.backward()\n",
    "\n",
    "print(a.grad)\n",
    "\n",
    "# Flush the gradients"
